<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="dev_count_down_snippet_assets" inherit_id="web_editor.assets_wysiwyg" name="Dev Count Down Snippet Assets">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/dev_count_down_snippet/static/src/js/options.js"></script>
        </xpath>
    </template>

	<template id="assets_snippet_dev_s_countdown_js_000" inherit_id="website.assets_frontend">
	    <xpath expr="//script[last()]" position="after">
	        <script type="text/javascript" src="/dev_count_down_snippet/static/src/js/000.js"/>
	    </xpath>
	</template>
</odoo>
